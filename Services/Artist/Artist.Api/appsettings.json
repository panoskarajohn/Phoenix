{
  "app": {
    "name": "Artist.Api",
    "service": "artist_api_service",
    "version": "1.0.0"
  },
  "logger": {
    "excludePaths": [
      "/ping",
      "/metrics"
    ],
    "level": "information",
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "hour"
    }
  },
  "CORS": {
    "main": [
      "http://host.docker.internal:4200"
    ]
  },
  "Otlp": {
    "enabled": true,
    "serviceName": "artist_api_service",
    "endpoint": "http://localhost:4317"
  },
  "mongo": {
    "connectionString": "mongodb://root:rootpassword@localhost:27017",
    "database": "artist_db",
    "seed": false
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": false,
    "name": "v1",
    "title": "Track.API",
    "version": "v1",
    "routePrefix": "docs",
    "includeSecurity": true
  }
}