{
  "app": {
    "name": "Track.Api",
    "service": "track_api_service",
    "version": "1.0.0"
  },
  "logger": {
    "excludePaths": [
      "/ping",
      "/metrics"
    ],
    "level": "information",
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "hour"
    }
  },
  "AllowedHosts": "*",
  "CORS": {
    "main": [
      "http://localhost:4200"
    ]
  },
  "Otlp": {
    "enabled": true,
    "serviceName": "Track.API",
    "endpoint": "http://otel"
  }
}